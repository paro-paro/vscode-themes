{
  "publisher": "paro-paro",
  "name": "paro-paro-themes",
  "displayName": "Paro Paro Themes",
  "type": "module",
  "version": "2.0.0",
  "description": "VS Code Themes by @paro-paro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paro-paro/vscode-themes.git"
  },
  "keywords": [
    "vscode",
    "theme",
    "night",
    "dark",
    "solarized",
    "paro-paro"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon/icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Paro Paro Night",
        "uiTheme": "vs-dark",
        "path": "./themes/night.json"
      },
      {
        "label": "Paro Paro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Paro Paro Solarized",
        "uiTheme": "vs-dark",
        "path": "./themes/solarized.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp && vsce publish --yarn"
  },
  "devDependencies": {
    "@paro-paro/eslint-config": "^1.2.0",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "8.57.0"
  }
}
